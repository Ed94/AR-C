Engine.Cycler.source


static : Persist : mut bool = false ;


Initialize :

	Persist = true

	loop :
	{
		if Persist != true : ret

		Timing  :: TakeSnapshot:: Start()
		Input   :: Update()
		State   :: Update()
		Renderer:: Update()
		Timing  :: TakeSnapshot:: End()
		Timing  :: Update()
	};
;


Lapse : Persist = false ;